{
  "manifest_version": 2,

  "name": "Quick Gradesource",
  "version": "1.1.0",

  "browser_action": {
    "default_icon": "images/icon38.png",
    "default_title": "Quick Gradesource", 
    "default_popup": "index.html"
  },

  "permissions": [
    "http://www.gradesource.com/*",
    "storage"
  ],

  "options_page": "options.html"
}