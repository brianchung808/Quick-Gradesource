{
  "manifest_version": 2,

  "name": "GS",
  "version": "0.0.1",
  /*"icons": {
    "128": "icon128.png"
  },
  */

  "browser_action": {
    "default_icon": {                    // optional
      //"19": "images/icon19.png",           // optional
      //"38": "images/icon38.png"            // optional
    },
    "default_title": "GS",      // optional; shown in tooltip
    "default_popup": "index.html"        // optional
  },

  "background": {
    "persistent": false,
    "scripts": ["jquery.min.js", "gradesource_scraper.js"]
  },

  "permissions": [
    "http://www.gradesource.com/*"
  ]
}
